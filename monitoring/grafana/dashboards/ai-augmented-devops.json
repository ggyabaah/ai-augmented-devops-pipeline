{
  "id": null,
  "title": "AI Augmented DevOps Dashboard",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 2,
  "refresh": "10s",
  "panels": [
    {
      "title": "Service Health - Trainer",
      "type": "stat",
      "targets": [
        { "expr": "up{job=\"trainer\"}" }
      ]
    },
    {
      "title": "Service Health - Tester",
      "type": "stat",
      "targets": [
        { "expr": "up{job=\"tester\"}" }
      ]
    },
    {
      "title": "Requests per Second",
      "type": "timeseries",
      "targets": [
        { "expr": "rate(model_training_requests_total[1m])", "legendFormat": "Trainer" },
        { "expr": "rate(model_test_requests_total[1m])", "legendFormat": "Tester" }
      ]
    },
    {
      "title": "Error Rate",
      "type": "timeseries",
      "targets": [
        { "expr": "rate(model_training_errors_total[5m])", "legendFormat": "Trainer Errors" },
        { "expr": "rate(model_test_errors_total[5m])", "legendFormat": "Tester Errors" }
      ]
    },
    {
      "title": "Latency (p95)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(request_latency_seconds_bucket{job=\"trainer\"}[5m]))",
          "legendFormat": "Trainer p95"
        },
        {
          "expr": "histogram_quantile(0.95, rate(request_latency_seconds_bucket{job=\"tester\"}[5m]))",
          "legendFormat": "Tester p95"
        }
      ]
    },
    {
      "title": "Model Accuracy",
      "type": "gauge",
      "targets": [
        { "expr": "model_training_accuracy", "legendFormat": "Trainer Accuracy" },
        { "expr": "model_test_accuracy", "legendFormat": "Tester Accuracy" }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "title": "Build Info",
      "type": "table",
      "targets": [
        { "expr": "model_build_info" }
      ]
    },
    {
      "title": "Uptime",
      "type": "stat",
      "targets": [
        { "expr": "time() - process_start_time_seconds{job=\"trainer\"}", "legendFormat": "Trainer Uptime" },
        { "expr": "time() - process_start_time_seconds{job=\"tester\"}", "legendFormat": "Tester Uptime" }
      ]
    }
  ]
}